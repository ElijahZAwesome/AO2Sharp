<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Title>AO2Sharp.Plugins.API</Title>
		<Description>The plugin framework for AO2Sharp, an Attorney Online server.</Description>
		<Summary>The plugin framework for AO2Sharp, an Attorney Online server.</Summary>
		<Copyright>Copyright 2020 Elijah Bansley</Copyright>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<Authors>ElijahZAwesome</Authors>
		<Company>ElijahZAwesome</Company>
		<Description>API For AO2Sharp plugins.</Description>
		<PackageProjectUrl>https://github.com/ElijahZAwesome/AO2Sharp</PackageProjectUrl>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>0.2</Version>
		<PackageLicenseExpression></PackageLicenseExpression>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

</Project>
